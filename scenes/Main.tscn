	# WALL NEUTRAL
	instanciate_wall(3,1,false)
	instanciate_wall(9,2,false)
	instanciate_wall(15,3,false)
	instanciate_wall(1,5,false)
	instanciate_wall(6,4,false)
	instanciate_wall(11,4,false)
